{
    "$schema": "https://json.schemastore.org/tsconfig",
    "_version": "22.0.0",
    "compilerOptions": {
        "lib": [
            "es2024",
            "ESNext.Array",
            "ESNext.Collection",
            "ESNext.Iterator"
        ],
        "target": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "outDir": "dist",
        "rootDir": "src",
    }
}